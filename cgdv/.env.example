# .env.example (переименуйте в .env и заполните своими данными)
# Токен бота от @BotFather
BOT_TOKEN=your_bot_token_here

# Ваш Telegram ID от @userinfobot
ADMIN_ID=your_telegram_id_here

# Настройки базы данных
DATABASE_PATH=data/gamebot.db

# Настройки приложения
DEBUG=False
LOG_LEVEL=INFO

# Лимиты
MAX_BIO_LENGTH=500
MAX_NAME_LENGTH=50
DAILY_LIKES_LIMIT=50
MATCHES_PER_REQUEST=1
MIN_AGE=16
MAX_AGE=99